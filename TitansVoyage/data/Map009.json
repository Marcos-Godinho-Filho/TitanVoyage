dent":0,"parameters":["When Mrs. Lauren warned us about long term exposure "]},{"code":401,"indent":0,"parameters":["to radiation, I didn't expect it to happen"]},{"code":401,"indent":0,"parameters":["to one of us..."]},{"code":101,"indent":0,"parameters":["lily",0,0,2]},{"code":401,"indent":0,"parameters":["No problem. *Cough, cough* "]},{"code":401,"indent":0,"parameters":["The mission has to continue."]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["But are you doing any sort of treatment,"]},{"code":401,"indent":0,"parameters":["taking medicine...?"]},{"code":101,"indent":0,"parameters":["lily",0,0,2]},{"code":401,"indent":0,"parameters":["How? We're almost in the middle of the trip,"]},{"code":401,"indent":0,"parameters":["there's no way to come back home now. "]},{"code":401,"indent":0,"parameters":["*Cough, cough*.\\|"]},{"code":401,"indent":0,"parameters":["Besides that, we are supply limited,"]},{"code":101,"indent":0,"parameters":["lily",0,0,2]},{"code":401,"indent":0,"parameters":["there’s no way wasting all of our resources"]},{"code":401,"indent":0,"parameters":["with me."]},{"code":101,"indent":0,"parameters":["lily",0,0,2]},{"code":401,"indent":0,"parameters":["Anyways, thanks for your *Cough*\\| concern."]},{"code":401,"indent":0,"parameters":["It’s good to have someone to talk to"]},{"code":401,"indent":0,"parameters":["these days."]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["Lily, if there's anything I can do,"]},{"code":401,"indent":0,"parameters":["let me know, you always can count on me."]},{"code":401,"indent":0,"parameters":["Now I have to fix our energy systems. "]},{"code":401,"indent":0,"parameters":["That blackout yesterday scared me..."]},{"code":221,"indent":0,"parameters":[]},{"code":230,"indent":0,"parameters":[60]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["**Some months later**"]},{"code":222,"indent":0,"parameters":[]},{"code":213,"indent":0,"parameters":[2,10,false]},{"code":101,"indent":0,"parameters":["ethan",0,0,2]},{"code":401,"indent":0,"parameters":["Hey Zack, did you notice Dave is acting weirdly"]},{"code":401,"indent":0,"parameters":["in the past days?"]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["Thank god it was not just me."]},{"code":401,"indent":0,"parameters":["Yesterday, I saw him talking nonsense alone"]},{"code":401,"indent":0,"parameters":["while doing research."]},{"code":401,"indent":0,"parameters":[" "]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["He’s going crazy with this years-long"]},{"code":401,"indent":0,"parameters":["confinement!"]},{"code":401,"indent":0,"parameters":["I think he developed a kind of psychiatric"]},{"code":401,"indent":0,"parameters":["disorder."]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["Maybe... I don’t know."]},{"code":401,"indent":0,"parameters":["I think we are all losing some level of"]},{"code":401,"indent":0,"parameters":["sanity here..."]},{"code":213,"indent":0,"parameters":[2,10,false]},{"code":101,"indent":0,"parameters":["ethan",0,0,2]},{"code":401,"indent":0,"parameters":["I agree..."]},{"code":401,"indent":0,"parameters":["I’d do anything to take some fresh air,"]},{"code":401,"indent":0,"parameters":["walk and eat a pad thai now."]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["I miss home too."]},{"code":401,"indent":0,"parameters":["At least we’re not alone..."]},{"code":213,"indent":0,"parameters":[2,1,false]},{"code":101,"indent":0,"parameters":["ethan",0,0,2]},{"code":401,"indent":0,"parameters":["Oh!"]},{"code":401,"indent":0,"parameters":["Dave woke up."]},{"code":101,"indent":0,"parameters":["dave",0,0,2]},{"code":401,"indent":0,"parameters":["... that moon rocks will taste like"]},{"code":401,"indent":0,"parameters":["raspberry jelly beans marinated in cosmic"]},{"code":401,"indent":0,"parameters":["wisdom while dancing with stardust ..."]},{"code":213,"indent":0,"parameters":[2,7,false]},{"code":213,"indent":0,"parameters":[-1,1,false]},{"code":213,"indent":0,"parameters":[1,1,false]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["Ops! It was just a dream!"]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["Hahahahahahahah!"]},{"code":221,"indent":0,"parameters":[]},{"code":230,"indent":0,"parameters":[60]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["**Two years later**"]},{"code":222,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["I need to inform the mission control about our"]},{"code":401,"indent":0,"parameters":["situation here."]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["*Talks on radio to send the message*"]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["T3023 to Earth,\\| T3023 to Earth,\\|"]},{"code":401,"indent":0,"parameters":["we have good and bad news."]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["The good news is Lily’s cancer had improved"]},{"code":401,"indent":0,"parameters":["last month. Say hi Lily."]},{"code":101,"indent":0,"parameters":["lily",0,0,2]},{"code":401,"indent":0,"parameters":["Hi!"]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["But the rest of the crew didn’t have"]},{"code":401,"indent":0,"parameters":["the same luck."]},{"code":101,"indent":0,"parameters":["imagemdorosto",0,0,2]},{"code":401,"indent":0,"parameters":["Ethan has developed Spaceflight associated"]},